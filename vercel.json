{
  "version": 2,

  "github": {
    "enable": false
  },
  "builds": [
    {
      "src": "/backend/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/frontend/public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
