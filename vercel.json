{
  "version": 2,

  "github": {
    "enable": false
  },
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "/frontend/build/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    { "src": "/(.*).(js|css|ico)", "dest": "/frontend/build/*" },
    { "src": "/(.*)", "dest": "/frontend/build/index.html" }
  ]
}
